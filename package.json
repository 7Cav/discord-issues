{
  "name": "discord-issues",
  "version": "0.0.1",
  "description": "Discord bot to manage github issues",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build/",
    "build": "tsc",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './build/index.js' --watch './build'",
    "start": "npm-run-all clean build --parallel watch:build watch:server --print-label"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7Cav/discord-issues.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/7Cav/discord-issues/issues"
  },
  "homepage": "https://github.com/7Cav/discord-issues#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.11",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@octokit/auth-app": "^2.4.6",
    "@octokit/rest": "^17.10.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "google-auth-library": "^6.0.1",
    "googleapis": "^52.1.0",
    "log4js": "^6.3.0",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "typescript-optional": "^2.0.1"
  }
}
